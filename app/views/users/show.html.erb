
<div id="user<%= @user.id %>-timeline">
</div>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<div class="stats">
 <%= render 'stats' %>
</div>
<br>
<br>

<% if current_user %>

<div class="follow-me">
  <%= render 'follow_form' %>
</div>

<% end %>

<div class="search_by_tag">
<%= form_tag tags_path, method: :get, id: "search_tag" do %>
  <%= text_field_tag :search, params[:search], :placeholder => "Enter any Tag" %>
  <%= submit_tag "Search" %>
<% end %>
</div>
<div class="search-tag-results"></div>
<br>
<br>

<div class="user-journey-list">
<% @user.owend_journeys.each do |journey| %>
  <div class="user-journey">
  <%= link_to journey.title, journey %>
  </div>
<% end %>
</div>

<br>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
