<%= form_tag user_sessions_path, :method => :post do %>
    <%= text_field_tag :email, nil, :placeholder => "Enter Your Email", class:"login-input"  %>
    <%= password_field_tag :password, nil, :placeholder => "Password", class:"login-input" %>
    <%= submit_tag "Login", :"data-no-turbolink" => true, class: "btn_login", onclick: "cambiar_login()"%>

  <%= label_tag :emember, "Remember me", class:"remeber-me" %> <%= check_box_tag :remember, params[:remember], true %>

<% end %>
